{
  "name": "@nlabs/gothamjs",
  "version": "0.18.5",
  "description": "Platform",
  "type": "module",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "browser": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "styles",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./icons": {
      "default": "./lib/icons/index.js",
      "import": "./lib/icons/index.js",
      "types": "./lib/icons/index.d.ts"
    },
    "./i18n": {
      "default": "./lib/i18n/index.js",
      "import": "./lib/i18n/index.js",
      "types": "./lib/i18n/index.d.ts"
    },
    "./router": {
      "default": "./lib/router/index.js",
      "import": "./lib/router/index.js",
      "types": "./lib/router/index.d.ts"
    },
    "./form": {
      "default": "./lib/form/index.js",
      "import": "./lib/form/index.js",
      "types": "./lib/form/index.d.ts"
    },
    "./styles/tailwind.css": {
      "default": "./lib/styles/tailwind.css"
    }
  },
  "keywords": [
    "arkhamjs",
    "documentation",
    "flux",
    "gothamjs",
    "markdown",
    "nitrogenlabs",
    "react"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/gothamjs.git"
  },
  "homepage": "http://gothamjs.io",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/gothamjs/issues"
  },
  "scripts": {
    "build": "lex compile --remove --config ./tsconfig.build.json && npm run sync:tailwind-lib",
    "build:dev": "NODE_ENV=development lex compile --remove --config ./tsconfig.build.json && npm run sync:tailwind-lib",
    "clean": "lex clean",
    "dev": "npm run sync:tailwind-lib && lex dev",
    "lint": "lex lint --fix",
    "prepublishOnly": "npm test && npm run build",
    "publish:major": "npm version major && npm run publish:tags && npm publish",
    "publish:minor": "npm version minor && npm run publish:tags && npm publish",
    "publish:patch": "npm version patch && npm run publish:tags && npm publish",
    "publish:tags": "git push --tags && git push origin HEAD",
    "start": "npm run sync:tailwind-lib && lex dev -r -o",
    "test": "lex test",
    "update": "lex update --interactive",
    "watch": "lex compile -w",
    "storybook": "npm run sync:tailwind-lib && lex storybook",
    "sync:tailwind-lib": "node -e \"const fs=require('fs'); fs.mkdirSync('./lib/styles',{recursive:true}); fs.copyFileSync('./src/styles/tailwind.css','./lib/styles/tailwind.css');\"",
    "tailwind:build": "npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./src/styles/index.css",
    "tailwind:watch": "npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./src/styles/index.css --watch"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "dependencies": {
    "@date-io/luxon": "3.2.0",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@nlabs/rip-hunter": "^3.1.0",
    "@nlabs/utils": "^2.8.6",
    "autosuggest-highlight": "^3.3.4",
    "cropperjs": "^2.1.0",
    "hast-util-sanitize": "5.0.2",
    "highlight.js": "^11.11.1",
    "history": "^5.3.0",
    "i18next": "^25.8.13",
    "lucide-react": "^0.575.0",
    "luxon": "^3.7.2",
    "react-autosuggest": "^10.1.0",
    "react-cropper": "^2.3.3",
    "react-hook-form": "^7.71.2",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-router": "^7.13.1",
    "react-tag-autocomplete": "^7.5.1",
    "react-transition-group": "^4.4.5",
    "rehype-react": "^8.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nlabs/arkhamjs": "^3.31.9",
    "@nlabs/arkhamjs-middleware-logger": "^3.30.6",
    "@nlabs/arkhamjs-storage-browser": "^3.30.6",
    "@nlabs/arkhamjs-utils-react": "^3.32.7",
    "@nlabs/lex": "1.55.0",
    "@types/history": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.3.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@nlabs/arkhamjs": "^3.31.4",
    "@nlabs/arkhamjs-middleware-logger": "^3.30.2",
    "@nlabs/arkhamjs-storage-browser": "^3.30.1",
    "@nlabs/arkhamjs-utils-react": "^3.32.3",
    "react": "*",
    "react-dom": "*"
  },
  "overrides": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "cross-spawn": "^7.0.3",
    "got": "^13.0.0",
    "http-cache-semantics": "^4.1.1",
    "http-proxy-middleware": "^3.0.3",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "semver-regex": "^4.0.5",
    "tmp": "^0.2.1",
    "cheerio": "^1.0.0",
    "lodash.pick": "npm:lodash@^4.17.21"
  }
}
